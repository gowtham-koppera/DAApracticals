#include <stdio.h>

int isprime(int n) {
    if (n == 2 || n == 3)
        return 1;
    if (n % 2 == 0 || n % 3 == 0)
        return 0;
    for (int i = 5; i * i <= n; i = i + 1) {
        if (n % i == 0)
            return 0;
    }
    return 1;
}

int main() {
    int n;
    printf("Enter no:");
    scanf("%d", &n);  // Fix: pass the address of n
    int ans = isprime(n);
    if (ans == 1)
        printf("prime no");
    else
        printf("not a prime no");  // Fix: added an else condition to handle non-prime case

    return 0;
}
